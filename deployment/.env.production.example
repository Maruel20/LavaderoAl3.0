# Ejemplo de archivo .env para producción
# Copia este archivo a .env y configura tus valores reales

# Base de Datos
DB_HOST=localhost
DB_USER=lavadero_user
DB_PASSWORD=TU_PASSWORD_SUPER_SEGURA_AQUI
DB_NAME=lavadero_al
DB_PORT=3306

# JWT - Genera una clave segura con: python3 -c "import secrets; print(secrets.token_urlsafe(32))"
SECRET_KEY=GENERA_UNA_CLAVE_SUPER_SEGURA_DE_MINIMO_32_CARACTERES
ALGORITHM=HS256

# CORS - Agrega tus dominios separados por comas (sin espacios)
ALLOWED_ORIGINS=https://tudominio.com,https://www.tudominio.com

# Opcional: Modo de producción
ENVIRONMENT=production
